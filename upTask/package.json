{
  "name": "uptask",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "12.18.2"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "cache:clear": "watchman watch-del-all && rm -rf node_modules && yarn cache clean && yarn install",
    "android:clear": "cd android && ./gradlew clean && cd ..",
    "ios:clear": "rm -rf ios/build && rm -f ios/Podfile.lock && cd ios && pod repo update && pod install && pod update",
    "rebuild": "yarn cache:clear && yarn ios:clear && yarn android:clear",
    "cache": "yarn start --reset-cache",
    "pod": "cd ios/ && pod install && cd ../"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-masked-view/masked-view": "^0.2.4",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "graphql": "^15.5.0",
    "native-base": "^2.15.2",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.1.1",
    "react-native-skeleton-placeholder": "^4.0.0",
    "react-native-vector-icons": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
